{
  "connstr": "mongodb://localhost/gatekeeper-dev",
  "port": 5000,

  "mongodb": {
    "db": {
      "native_parser": true
    },
    "server": {
      "auto_reconnect": true,
      "keepAlive": 1,
      "poolSize": 5
    }
  },
  "morgan": {
    "format": "dev",
    "immediate": true
  },
  "session": {
    "cookie": {
      "secure": true
    },
    "secret": "123abc",
    "resave": true,
    "saveUninitialized": true
  }
}